package com.falldetection.model;

public class PatientInfo {
	
	private String patientName;
	private String password;
	private String defaultAddress;
	private int emergencyHospital;
	private int emergencyContact;
	private long patientsId;
	
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	
	public long getPatientsId() {
		return patientsId;
	}
	public String getPatientName() {
		return patientName;
	}
	public void setPatientName(String patientName) {
		this.patientName = patientName;
	}
	public String getDefaultAddress() {
		return defaultAddress;
	}
	public void setDefaultAddress(String defaultAddress) {
		this.defaultAddress = defaultAddress;
	}
	public int getEmergencyHospital() {
		return emergencyHospital;
	}
	public void setEmergencyHospital(int emergencyHospital) {
		this.emergencyHospital = emergencyHospital;
	}
	public int getEmergencyContact() {
		return emergencyContact;
	}
	public void setEmergencyContact(int emergencyContact) {
		this.emergencyContact = emergencyContact;
	}
	public void setPatientsId(long patientsId) {
		this.patientsId = patientsId;
	}
	
}
